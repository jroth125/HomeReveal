{
    "name": "HomeReveal",
    "description": "An extension to reveal where you want to live",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Chrome Extension Boilerplate",
        "default_popup": "../static/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ]
  }
  